<div class="alert alert-info"><%= __('Results') %>:  <%= teams.length %></div>
<% for(var i=0; i < teams.length; i++) { %>
<div class="col-lg-3 col-sm-6">
    <div class="card hovercard">
        <div class="cardheader"></div>
        <div class="avatar">
            <img alt="" src="/images/uploads/<%= teams[i].logo %>">
        </div>
        <div class="info">
            <div class="title">
                <a href="/times/<%= teams[i].slug %>"><%= teams[i].alias %></a>
            </div>
            <div class="subtitle">
                <a href="/times/<%= teams[i].slug %>"><%= teams[i].name %></a>
            </div>
        </div>
        <div class="bottom player_teams">
            <a href="/times/editar/<%= teams[i].id %>">Editar</a>
        </div>
    </div>
</div>
<% } %> 