<div id="contato" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Contato</h4>
            </div>
            <form id="form_contato" method="POST" role="form" data-toggle="validator">
                <div class="modal-body">
                    
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label for="name" class="control-label"><%= __('Name') %></label>
                                    <input type="name" class="form-control" id="name" name="name" placeholder="" data-error="<%= __('Please, fill the form with a valid name') %>" required>
                                    <div class="help-block with-errors"></div>
                                </div>
                            </div>    
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label for="email" class="control-label"><%= __('Email') %></label>
                                    <input type="email" class="form-control" id="email" name="email" placeholder="" data-error="<%= __('Please, fill the form with a valid email') %>" required>
                                    <div class="help-block with-errors"></div>
                                </div>
                            </div> 
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label for="title" class="control-label"><%= __('Subject') %></label>
                                    <select type="title" class="form-control" id="title" name="title" placeholder="" data-error="<%= __('Please, fill the form with a valid title') %>" required>
                                        <option value="[Geral] Ajuda">[Geral] Ajuda</option>
                                        <option value="[Conteúdo] Informar conteúdo indesejável">[Conteúdo] Informar conteúdo indesejável</option>
                                        <option value="[Marketing] Anunciar no Portal">[Marketing] Anunciar no Portal</option>
                                        <option value="[Perfil] Solicitar alteração do e-mail cadastrado">[Perfil] Solicitar alteração do e-mail cadastrado</option>
                                        <option value="[Times] Solicitar exclusão ou inativação em equipe">[Times] Solicitar exclusão ou inativação em equipe</option>
                                        <option value="[Times] Solicitar alteração do responsável">[Times] Solicitar alteração do responsável</option>
                                        
                                    </select>
                                    <div class="help-block with-errors"></div>
                                </div>
                            </div> 
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <label for="message" class="control-label"><%= __('Message') %></label>
                                    <textarea type="message" class="form-control" id="message" name="message" placeholder="" data-error="<%= __('Please, fill the form with a valid message') %>"></textarea>
                                    <div class="help-block with-errors"></div>
                                </div>
                            </div> 
                        </div>
                       
                </div>
                <div class="modal-footer">
                    <a href="#" data-dismiss="modal" class="btn btn-default"><%= __('Cancel') %></a>
                    <button type="submit" class="btn btn-default btn-primary"><%= __('Send') %></button>
                </div>
            </form>
        </div>
    </div>
</div> 


